{
  "name": "ng-utils",
  "version": "0.0.2",
  "type": "module",
  "peerDependencies": {
    "@angular/common": "^17.3.0",
    "@angular/core": "^17.3.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "module": "./fesm2022/core.mjs",
  "typings": "./core.d.ts",
  "exports": {
    "./schematics/*": {
      "default": "./schematics/*.js"
    },
    "./package.json": {
